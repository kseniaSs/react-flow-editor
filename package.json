  },
  "dependencies": {
    "classnames": "^2.2.6",
    "lodash": "^4.17.21",
    "react": "^16.13.1",
    "react-dom": "^16.14.0",
    "recoil": "^0.7.4"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0",
    "react-dom": "^16.8.0 || ^17.0.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.10",
    "@types/react": "^16.9.49",
    "@vitejs/plugin-react": "^1.3.2",
    "copyfiles": "^2.3.0",
    "prettier": "^2.7.1",
    "sass": "^1.53.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.2",
    "vite": "^2.9.13",
    "vite-plugin-dts": "^1.2.1",
